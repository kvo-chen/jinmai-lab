{
  "name": "project_template_react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:client": "vite --host --port 3000",
    "dev:server": "node --experimental-modules api/server.mjs",
    "dev:llm": "node server.js",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\" \"pnpm dev:llm\"",
    "build:client": "vite build --outDir dist/static",
    "build": "pnpm build:client && node -e \"const fs=require('fs'); const path=require('path'); const outDir=path.join('dist','static'); fs.mkdirSync('dist',{recursive:true}); fs.copyFileSync('package.json','dist/package.json'); const ver=JSON.parse(fs.readFileSync('package.json','utf-8')).version; const stamp=new Date().toISOString(); fs.writeFileSync(path.join(outDir,'version.txt'), `Build Time: ${stamp}\\nVersion: ${ver}\\nBuild: SUCCESS\\n`);\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^12.9.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.2",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
