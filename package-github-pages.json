{
  "name": "jinmai-lab-github-pages",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:client": "vite --host --port 3000",
    "dev": "vite --host --port 3000",
    "build:github-pages": "vite build --outDir dist --base /jinmai-lab/",
    "build": "pnpm build:github-pages && node -e \"const fs=require('fs'); const path=require('path'); const outDir='dist'; try { const pkg=JSON.parse(fs.readFileSync('package.json','utf-8')); const ver=pkg.version || '0.0.0'; const stamp=new Date().toISOString(); fs.writeFileSync(path.join(outDir,'version.txt'), `Build Time: ${stamp}\\nVersion: ${ver}\\nBuild: SUCCESS\\n`); } catch(e) { fs.writeFileSync(path.join(outDir,'version.txt'), `Build Time: ${new Date().toISOString()}\\nVersion: 0.0.0\\nBuild: SUCCESS\\n`); }\"",
    "preview": "vite preview --port 3000"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^12.10.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.3",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.1",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}